<article class="box post" id="box-post">
    {{ form_start(form) }}
    <div style="padding-bottom: 5%">
        {{ form_label(form.priceName, 'Nom du document') }}
        {{ form_widget(form.priceName) }}
    </div>
    <div>
        {{ form_row(form.priceContent, { 'id': 'editor' }, {'attr': {'class': 'editor'}}) }}
        <button class="btn alt2">{{ button_label|default('Enregistrer') }}</button>
    </div>
    {{ form_end(form) }}
</article>



